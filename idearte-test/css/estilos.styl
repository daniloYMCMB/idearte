/* Estilos del formulario */
form.contacto {
    margin: 0 auto 0
    width: 440px
}
form.contacto fieldset {

}
form.contacto fieldset > div {
    margin-bottom: 12px
    overflow: hidden
}
form.contacto fieldset div label {
    display: block
    float: left
    margin-right: 15px
    margin-top: 5px
    text-align: right
    text-transform: uppercase
    width: 84px
}
form.contacto input[type="text"], form.contacto textarea {
    background: rgba(255, 255, 255, 0)
    border: 1px solid #fff200
    border-radius: 4px
    color: white
    display: block
    float: left
    padding: 8px
    resize: none
    width: 100%
    -webkit-transition: all 0.1s linear
    -moz-transition: all 0.1s linear
}
form.contacto input[type="text"]:focus, form.contacto textarea:focus {
    background: rgba(0, 0, 0, 0.1)
    border: 1px solid #fff200
    color: #ffd500
    outline: none
}
form.contacto .ultimo{
    margin-bottom: 0
    position: relative
}
form.contacto button {
    background: rgba(255, 255, 255, 0)
    border: 1px solid #fff200
    color: white
    float: right
    font-size: 1.7rem
    border-radius: 4px
    font-family: "europa","Helvetica"
    height: 32px
    font-weight: 900
    padding: 6px 40px 28px 40px
    position: relative
    -webkit-transition: all 0.2s linear
    -moz-transition: all 0.2s linear
}
form.contacto button:hover{
    background: #FFE000
}


